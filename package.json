{
  "name": "snyk-resolve",
  "description": "Resolves the location of a node package given a path",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "format": "prettier --write lib/*.?s test/*.?s",
    "lint": "eslint lib/*.js test/*.?s",
    "test": "npm run lint && tap test/*.test.js --cov --timeout=60"
  },
  "keywords": [
    "snyk"
  ],
  "author": "Remy Sharp",
  "license": "Apache-2.0",
  "dependencies": {
    "debug": "^3.1.0",
    "then-fs": "^2.0.0"
  },
  "devDependencies": {
    "es6-promise": "^4.2.8",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "prettier": "^1.18.2",
    "proxyquire": "^2.1.3",
    "semantic-release": "^15.13.24",
    "tap": "^12.7.0",
    "tape": "^4.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Snyk/resolve-package.git"
  }
}
